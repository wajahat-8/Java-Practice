package unionfind;

import java.util.Arrays;

public class ArrayUtilityExample {
	public static void main(String[] args) {
		int[] intArray= {3,1,2};
		Arrays.sort(intArray);
		System.out.println("Sorted Array: "+Arrays.toString(intArray));
		int index=Arrays.binarySearch(intArray, 2);
		System.out.println("Index of element '2' in sorted array: "+ index);
		Arrays.fill(intArray, 5);
		System.out.println("Arrays after filling with 5:"+Arrays.toString(intArray));
		Integer [] boxedArray= {1,2,3};
		java.util.List<Integer> list=Arrays.asList(boxedArray);
		System.out.println("Array converted to List: " + list);
	}
	

}

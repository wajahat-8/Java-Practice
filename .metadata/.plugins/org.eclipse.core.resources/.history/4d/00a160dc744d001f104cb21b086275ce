package unionfind;

import java.util.Arrays;

public class ArrayUtilityExample {
	public static void main(String[] args) {
		int[] intArray= {3,1,2};
		Arrays.sort(intArray);
		System.out.print("Sorted Array: "+Arrays.toString(intArray));
		int index=Arrays.binarySearch(intArray, 2);
		System.out.println("Index of element '2' in sorted array: "+ index);
	}
	

}
